{
  "presets": ["es2015", "react", "stage-0"],
  "env": {
    "development": {
      "plugins": ["babel-relay-plugin-loader", "transform-class-properties", "transform-decorators-legacy"],
      "ignore": ['**/lib/**', '**/node_modules/**'],
      compact: false
    },
    "production": {
      "plugins": ["transform-class-properties", "babel-relay-plugin-loader", "transform-decorators-legacy"],
      "ignore": ['**/lib/**', '**/node_modules/**'],
      compact: false 
    }
  }
}
